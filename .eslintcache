arItems];\n}\n\nexport default useFbStorage;","/home/ec2-user/environment/itss_std_react_todo_k62/src/lib/firebase.js",[],{"ruleId":"62","replacedBy":"63"},{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","severity":1,"message":"67","line":1,"column":17,"nodeType":"68","messageId":"69","endLine":1,"endColumn":25},{"ruleId":"66","severity":1,"message":"70","line":13,"column":9,"nodeType":"68","messageId":"69","endLine":13,"endColumn":15},{"ruleId":"71","severity":1,"message":"72","line":21,"column":42,"nodeType":"73","messageId":"74","endLine":21,"endColumn":44},{"ruleId":"75","severity":1,"message":"76","line":14,"column":6,"nodeType":"77","endLine":18,"endColumn":9},{"ruleId":"75","severity":1,"message":"76","line":20,"column":6,"nodeType":"77","endLine":24,"endColumn":9},{"ruleId":"75","severity":1,"message":"76","line":26,"column":6,"nodeType":"77","endLine":30,"endColumn":9},{"ruleId":"62","replacedBy":"78"},{"ruleId":"64","replacedBy":"79"},{"ruleId":"71","severity":1,"message":"80","line":36,"column":20,"nodeType":"73","messageId":"81","endLine":36,"endColumn":22},"no-native-reassign",["82"],"no-negated-in-lhs",["83"],"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","'getKey' is defined but never used.","array-callback-return","Array.prototype.filter() expects a value to be returned at the end of arrow function.","ArrowFunctionExpression","expectedAtEnd","jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/evcohen/eslint-plugin-jsx-a11y/blob/master/docs/rules/anchor-is-valid.md","JSXOpeningElement",["82"],["83"],"Array.prototype.map() expects a return value from arrow function.","expectedInside","no-global-assign","no-unsafe-negation"]